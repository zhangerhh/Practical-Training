<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.auth.mapper.MenuMapper">
    <select id="findMenuByPermission" parameterType="String" resultType="com.demo.commons.domain.Permission_Menu">
        select * from permission_menu where permission_id = #{permission_id}
    </select>

</mapper>