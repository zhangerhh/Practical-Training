<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.auth.mapper.PermissionMapper">
    <select id="findPermissionByRole" parameterType="String" resultType="com.demo.commons.domain.Role_Permission">
        select * from role_permission where role = #{role}
    </select>

</mapper>